class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        ref = ""
        shortlen = len(min(strs, key=len))
        strs.sort()
        for i in range(shortlen):
            if strs[0][i] == strs[-1][i]:
                ref += strs[0][i]
            else:
                break
        return ref
