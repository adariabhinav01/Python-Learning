class Solution:
    def lengthOfLastWord(self, s: str) -> int:
        ll = 0
        fl = 0

        for i in range(len(s)-1,-1,-1):
            if s[i].isalpha():
                ll = i+1
                print(ll)
                break
        
        for i in range(ll-1,-1,-1):
            if s[i] == " ":
                fl = i
                break
            if i==0:
                fl = i-1
                break
        
        result = ll-fl-1
        return result
